alias gps_init='sudo modprobe garmin_gps && gpsd -S 4000 /dev/sensors/garmin_gps'
alias gpsd_init='gpsd -S 4000 /dev/sensors/garmin_gps'
alias gpscat_init='gpscat -s 19200 /dev/sensors/garmin_gps'
alias gps_ros='rosrun gpsd_client gpsd_client _port:=4000'

alias phx_setup='xhost +si:localuser:root && cd ~/phoenix_ws/src/phoenix-r1 && catkin-docker run'
alias save_phx_map='rosrun map_server map_saver map:=/walrus/point_cloud_cache/renderers/full_map'
alias save_phx_map_jackal='rosrun map_server map_saver map:=/jackal/point_cloud_cache/renderers/full_map'
alias save_map='rosrun map_server map_saver map:=/walrus/gmap'

