# Git
alias gitfabian='git config --global user.name FabianEP11 && git config --global user.email fabianparra@utexas.edu'

# Local Network
alias jackalmaster='export ROS_MASTER_URI=http://jackal:11311 && export ROS_IP=192.168.131.1'

# AugRE Network
alias m17nrgmaster='export ROS_MASTER_URI=http://m17_nrg:11311 && export ROS_IP=192.168.11.131'
alias nuvonrgmaster='export ROS_MASTER_URI=http://nuvo_nrg:11311 && export ROS_IP=192.168.11.131'
alias jackalnrgmaster='export ROS_MASTER_URI=http://jackal_nrg:11311 && export ROS_IP=192.168.11.131'
alias philbartnrgmaster='export ROS_MASTER_URI=http://philbart_nrg:11311 && export ROS_IP=192.168.11.131'
alias vaultbotnrgmaster='export ROS_MASTER_URI=http://vaultbot_nrg:11311 && export ROS_IP=192.168.11.131'

# CDU Uav Network
alias cdumaster='export ROS_MASTER_URI=http://192.168.1.101:11311 && export ROS_IP=192.168.1.236'

# Velodyne
alias velodyne_connect="sudo ifconfig br0:1 192.168.1.100"

# Bluetooth PS4 Controller
alias ps4_reconnect="bluetoothctl trust 90:89:5F:6D:55:64 && bluetoothctl disconnect 90:89:5F:6D:55:64 && bluetoothctl connect 90:89:5F:6D:55:64 && bluetoothctl trust 90:89:5F:6D:55:64"

# Temoto
alias stemoto='source ~/jackal_temoto/devel/setup.bash'

# Robotfleet
alias robofleet='ROS_NAMESPACE="2D_jackalnrg" make run'
alias samrl='export ROS_PACKAGE_PATH=/home/administrator/hololens_ws/src/robofleet_client/amrl_msgs:$ROS_PACKAGE_PATH'

# Cuda
alias cuda10='export CUDA_HOME="/usr/local/cuda-10.0" && export CD_LIBRARY_PATH="/usr/local/cuda-10.0/bin:$PATH" && export PATH="/usr/local/cuda-10.0/bin:$PATH"'

# OpenVPN
alias startvpn='openvpn3 session-start --config ~/Jackal_blake@ut-nrg.openvpn.com_[Dallas_TX].ovpn'
alias stopvpn='openvpn3 session-manage --disconnect --config ~/Jackal_blake@ut-nrg.openvpn.com_[Dallas_TX].ovpn'

# Clock
alias reset_clock_walrus='/etc/init.d/chrony stop && sudo ntpdate 192.168.0.101 && /etc/init.d/chrony start'
alias reset_clock_nrg='/etc/init.d/chrony stop && sudo ntpdate 192.168.11.101 && /etc/init.d/chrony start'

# Demo launches
alias phoenix='phxlaunch phoenix_launch jackal.xlaunch'
alias augre='roslaunch afc_demos full_demo.launch robot:=jackal use_database:=true'
alias augreo='roslaunch afc_demos full_demo.launch robot:=jackal use_database:=true enable_detection:=true'
alias augrec='roslaunch afc_demos full_demo.launch robot:=jackal use_database:=true enable_detection:=true enable_cot:=true'
alias augred='roslaunch afc_demos full_demo.launch robot:=jackal use_database:=true enable_detection:=true enable_darknet:=true'
alias augrer='roslaunch afc_demos full_demo.launch robot:=jackal use_database:=true enable_detection:=true enable_retinanet:=true'
alias objdet='roslaunch object_detection object_detection.launch robot:=jackal'
alias objdetd='roslaunch object_detection object_detection.launch robot:=jackal enable_darknet:=true'
alias objdetr='roslaunch object_detection object_detection.launch robot:=jackal enable_retinanet:=true'
