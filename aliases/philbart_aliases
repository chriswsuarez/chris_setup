# Cuda
alias cuda10='export CUDA_HOME="/usr/local/cuda-10.0" && export CD_LIBRARY_PATH="/usr/local/cuda-10.0/bin:$PATH" && export PATH="/usr/local/cuda-10.0/bin:$PATH"'

# Clock
alias reset_clock_walrus='/etc/init.d/chrony stop && sudo ntpdate 192.168.0.101 && /etc/init.d/chrony start'
alias reset_clock_nrg='/etc/init.d/chrony stop && sudo ntpdate 192.168.11.101 && /etc/init.d/chrony start'

# Demo launches
alias phoenix='phxlaunch phoenix_launch philbart.xlaunch'
alias augre='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true'
alias augreo='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true enable_detection:=true'
alias augrec='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true enable_detection:=true enable_cot:=true'
alias augred='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true enable_detection:=true enable_darknet:=true'
alias augrer='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true enable_detection:=true enable_retinanet:=true'
alias objdet='roslaunch object_detection object_detection.launch robot:=philbart'
alias objdetd='roslaunch object_detection object_detection.launch robot:=philbart enable_darknet:=true'
alias objdetr='roslaunch object_detection object_detection.launch robot:=philbart enable_retinanet:=true'

# NAT
alias nattt='sudo iptables -t nat -A POSTROUTING -o wlp16s0 -j MASQUERADE && sudo iptables -A FORWARD -i br0 -o wlp16s0 -j ACCEPT && sudo iptables -A FORWARD -i wlp16s0 -o br0 -m state --state RELATED,ESTABLISHED -j ACCEPT'
# alias naddd='sudo iptables -t nat -A POSTROUTING -o br0 -j MASQUERADE && sudo iptables -A FORWARD -i wlp16s0 -o br0 -m state --state RELATED,ESTABLISHED -j ACCEPT'
alias nawww='sudo iptables -A FORWARD -i wlp16s0 -o br0 -m state --state RELATED,ESTABLISHED -j ACCEPT'