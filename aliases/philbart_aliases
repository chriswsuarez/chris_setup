# Cuda
alias cuda10='export CUDA_HOME="/usr/local/cuda-10.0" && export CD_LIBRARY_PATH="/usr/local/cuda-10.0/bin:$PATH" && export PATH="/usr/local/cuda-10.0/bin:$PATH"'

# Clock
alias reset_clock_augre='/etc/init.d/chrony stop && sudo ntpdate 192.168.11.11 && /etc/init.d/chrony start'

# Demo launches
alias phoenix='phxlaunch phoenix_launch philbart.xlaunch'
alias phxteleop='source ~/phoenix/phoenix-r1/install/setup.bash && roslaunch ~/phoenix/phoenix-r1/philbart_teleop.launch'
alias augre='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true'
alias augred='roslaunch afc_demos full_demo.launch robot:=philbart use_database:=true enable_detection:=true'
alias objdet='roslaunch afc_demos object_detection.launch robot:=philbart'
alias oakd_converion='roslaunch oakd_camera darknet_conversion.launch robot:=philbart'

# Demo calls
alias resetna='rosservice call /philbart/robot_asa_node/reset_nearest_anchor "{}"'

# NAT
alias nattt='sudo iptables -t nat -A POSTROUTING -o wlp16s0 -j MASQUERADE && sudo iptables -A FORWARD -i br0 -o wlp16s0 -j ACCEPT && sudo iptables -A FORWARD -i wlp16s0 -o br0 -m state --state RELATED,ESTABLISHED -j ACCEPT'
